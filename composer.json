{
    "name": "sam/sync-package",
    "description": "A Laravel package to sync local data to cloud databases.",
    "keywords": ["laravel", "sync", "cloud", "database", "package"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Shahab Alam",
            "email": "your.email@example.com",
            "homepage": "https://github.com/shahabjamali45"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Sam\\SyncPackage\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sam\\SyncPackage\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sam\\SyncPackage\\SyncServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
